                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.QuizActivity"
            android:theme="@style/NoTittle">

        </activity>
    </application>

</manifest>